package Master;

public class MasterVo {
	private int mas_seq;
	private int mem_seq;
	private String id;
	private String pwd;
	
	public MasterVo() {
		
	}
	
	public MasterVo(String id, String pwd) {
		super();
		this.id = id;
		this.pwd = pwd;
	}
	
	public int getMas_seq() {
		return mas_seq;
	}
	public void setMas_seq(int mas_seq) {
		this.mas_seq = mas_seq;
	}
	public int getMem_seq() {
		return mem_seq;
	}
	public void setMem_seq(int mem_seq) {
		this.mem_seq = mem_seq;
	}
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getPwd() {
		return pwd;
	}
	public void setPwd(String pwd) {
		this.pwd = pwd;
	}
	@Override
	public String toString() {
		return "MasterVo [mas_seq=" + mas_seq + ", mem_seq=" + mem_seq + ", id=" + id + ", pwd=" + pwd + "]";
	}
	
}
